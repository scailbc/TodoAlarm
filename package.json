{
  "name": "TodoAlarm",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --coverage",
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "build": "npm run clean && npm run tsc --",
    "lint": "tslint src/**/*.ts",
    "watch": "npm run build -- -w",
    "start:ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
    "start:android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'"
  },
  "dependencies": {
    "native-base": "2.3.8",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "react-native-i18n": "2.0.10",
    "react-native-tts": "1.4.1",
    "react-native-typescript-transformer": "1.2.3",
    "react-navigation": "2.3.1",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-persist": "5.9.1",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@types/jest": "22.0.1",
    "@types/react": "16.0.34",
    "@types/react-native": "0.52.1",
    "babel-jest": "22.1.0",
    "babel-preset-react-native": "4.0.0",
    "concurrently": "3.5.1",
    "jest": "22.1.4",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
